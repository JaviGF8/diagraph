{
  "$schema": "http://json.schemastore.org/prettierrc",
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "singleQuote": true,
  "useTabs": false,
  "printWidth": 120,
  "tabWidth": 2,
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "noSemi": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "arrowParens": "always"
}
